{
  "name": "master-business-operations-dashboard",
  "version": "1.0.0",
  "description": "Master Business Operations Dashboard for A.M. Consulting, The 7 Space, and HigherSelf Network",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "echo 'Backend build completed'",
    "build:production": "NODE_ENV=production npm run build",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "cypress run",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:performance": "artillery run tests/performance/load-test.yml",
    "test:security": "npm audit && snyk test",
    "migrate:production": "node database/migrate.js",
    "seed:organizations": "node database/seed.js",
    "db:optimize": "node database/optimize.js",
    "setup:tenants": "node scripts/setup-tenants.js",
    "init:am-consulting": "node scripts/init-am-consulting.js",
    "init:seven-space": "node scripts/init-seven-space.js",
    "init:higherself": "node scripts/init-higherself.js",
    "optimize:assets": "node scripts/optimize-assets.js",
    "generate:sw": "workbox generateSW workbox-config.js",
    "analyze:bundle": "webpack-bundle-analyzer frontend/dist/static/js/*.js",
    "health-check": "node scripts/health-check.js",
    "db:test-connection": "node scripts/test-db-connection.js",
    "memory:profile": "node --inspect scripts/memory-profile.js",
    "integrations:test": "node tests/integrations/test-all.js",
    "auth:test": "node tests/auth/test-auth.js",
    "performance:baseline": "node tests/performance/baseline.js",
    "docker:build": "docker build -t master-dashboard:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "deploy": "node scripts/deploy.js",
    "backup": "node scripts/backup.js",
    "monitor": "node scripts/monitor.js"
  },
  "keywords": [
    "dashboard",
    "operations",
    "business",
    "monitoring",
    "analytics",
    "multi-tenant"
  ],
  "author": "HigherSelf Network",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "mysql2": "^3.6.0",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "winston": "^3.10.0",
    "node-cron": "^3.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.1",
    "nodemailer": "^6.9.3",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "csv-parser": "^3.0.0",
    "pdf-parse": "^1.1.1",
    "archiver": "^5.3.1",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "cypress": "^12.17.1",
    "artillery": "^2.0.0",
    "snyk": "^1.1188.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "workbox-cli": "^7.0.0",
    "@types/node": "^20.4.2",
    "eslint": "^8.44.0",
    "prettier": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/master-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/master-dashboard/issues"
  },
  "homepage": "https://github.com/your-org/master-dashboard#readme"
}


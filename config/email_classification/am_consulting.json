  "business_rules": [
    {
      "rule": "domain_exact_match",
      "patterns": ["amconsulting.com", "am-consulting.com"],
      "confidence_boost": 0.4,
      "description": "Direct domain match for AM Consulting"
    },
    {
      "rule": "business_keywords_threshold",
      "threshold": 3,
      "confidence_boost": 0.3,
      "description": "Multiple business consulting keywords present"
    },
    {
      "rule": "executive_sender",
      "patterns": ["ceo", "president", "director", "executive", "partner"],
      "confidence_boost": 0.2,
      "description": "Email from executive-level sender"
    },
    {
      "rule": "proposal_subject",
      "patterns": ["proposal", "consultation", "business opportunity"],
      "confidence_boost": 0.25,
      "description": "Business proposal or consultation in subject"
    }
  ],
  "exclusion_rules": [
    {
      "rule": "art_gallery_keywords",
      "patterns": ["art", "gallery", "exhibition", "artist", "artwork"],
      "confidence_penalty": 0.5,
      "description": "Contains art gallery related content"
    },
    {
      "rule": "wellness_keywords", 
      "patterns": ["wellness", "meditation", "yoga", "healing", "therapy"],
      "confidence_penalty": 0.5,
      "description": "Contains wellness center related content"
    },
    {
      "rule": "personal_domains",
      "patterns": ["gmail.com", "yahoo.com", "hotmail.com", "outlook.com"],
      "confidence_penalty": 0.2,
      "description": "Personal email domain reduces business confidence"
    }
  ],
  "workflow_triggers": [

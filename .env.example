# ==== REDIS CONFIGURATION ====
# Redis connection URI (Redis Cloud endpoint)
REDIS_URI=redis://redis-18441.c280.us-central1-2.gce.redns.redis-cloud.com:18441
# Redis password (required for Redis Cloud)
REDIS_PASSWORD=your_redis_password_here
# Redis connection timeout in seconds
REDIS_TIMEOUT=5
# Maximum number of connections in the pool
REDIS_MAX_CONNECTIONS=10
# Enable Redis cache (true/false)
REDIS_CACHE_ENABLED=true
# Enable SSL/TLS for Redis connection (true/false)
REDIS_SSL=true
# Enable Redis integration (true/false)
ENABLE_REDIS=true

